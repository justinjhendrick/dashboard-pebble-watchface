{
  "name": "dashboard",
  "author": "justinjhendrick, SocketMix",
  "version": "1.0.0",
  "keywords": [
    "pebble-watchface",
    "digital"
  ],
  "private": false,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "displayName": "Dashboard",
    "uuid": "658a0530-fdbb-4573-a047-303111dee408",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite",
      "emery"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "color_background",
      "color_time_text",
      "color_corner_title",
      "color_corner_value",
      "color_separator",
      "include_seconds",
      "month_first"
    ],
    "resources": {
      "media": [
        {"name": "ROBOTO_48", "file": "fonts/Roboto-Regular.ttf", "characterRegex": "[0-9:]", "type": "font"},
        {"name": "ROBOTO_36", "file": "fonts/Roboto-Regular.ttf", "characterRegex": "[0-9:]", "type": "font"},
        {"name": "ROBOTO_24", "file": "fonts/Roboto-Regular.ttf", "characterRegex": "[0-9:]", "type": "font"}
      ]
    }
  }
}
